@model IEnumerable<AppQuotation.Models.Company>
@{
    ViewBag.Title = "Все Компании";
}
<a asp-action="Create">Добавить компанию</a>
<table class="table">
    <tr><td>Название</td><td>Тикер</td><td></td></tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.Name</td>
    <td>@item.Ticker</td>
    <td>
        <a asp-action="Edit" asp-controller="Company" asp-route-id="@item.Id">Изменить</a> |
        <a asp-action="Delete" asp-controller="Company" asp-route-id="@item.Id">Удалить</a>
    </td>
</tr>
    }
</table>