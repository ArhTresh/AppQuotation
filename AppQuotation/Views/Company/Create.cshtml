@model AppQuotation.Models.Company
@{
    ViewBag.Title = "Добавление компании";
}
<form asp-action="create" asp-controller="Company">
    <div class="form-group">
        <label asp-for="Name" class="control-label">Название</label>
        <input type="text" asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Ticker" class="control-label">Тикер</label>
        <input type="text" asp-for="Ticker" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>